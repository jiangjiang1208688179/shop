
.navBar{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.homeMenus,.goodItem{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: left;
  padding: 8rpx;
  margin-top:20rpx;
  margin-bottom: 20rpx;
}
.homeMenusItem{
  display: flex;
  flex-direction: column;
  padding: 12rpx;
  text-align: center;
  width:155rpx;
  height:190rpx;
}
.sellGoods{
  padding: 20rpx;
}
.goodTitle{
  font-size: 42rpx;
  padding-bottom: 5rpx;
  margin-top:15rpx;
}
.newGoodsItem{
  margin-top:20rpx;
  margin-bottom: 20rpx; 
}
.newGoodsItemName{
  font-weight: 500;
}
.newGoodsItemActive{
  font-size: 25rpx;
  color: #6A5ACD;
}
.textColor{
  color: #8a8a8a;
  font-size: 35rpx;
}
.goodItem{
  justify-content: space-between;
  padding: 0rpx;
}
