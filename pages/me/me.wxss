.mycount{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 300rpx;
  background: #d4237a;
}
.mycount image{
  width: 120rpx;
  height: 120rpx;
}
.mycount text{
  margin-top: 20rpx;
  font-size: 30rpx;
  color: white;
}
.content{
  margin: 20rpx;
  color: #8e8e8e;
}
.showOrderList{
  display: flex;
  justify-content: space-between;
  font-size: 35rpx;
}
.goodsStatus{
  display: flex;
  padding: 10rpx;
  margin-top:20rpx;
  margin-bottom: 20rpx; 
  justify-content: space-between;
}
.goodsStatusItem{
  display: flex;
  flex-direction: column;
  text-align: center;
  width: 140rpx;
  font-size: 28rpx;
}
.goodsStatusItem image{
  width: 60rpx;
  height: 60rpx;
}
.someInfor{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 30rpx;
  margin-top:20rpx;
}
.someInfor image{
  width: 40rpx;
  height: 40rpx;
}
.myOther{
  width: 90%;
  padding: 10rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #ccc;
}